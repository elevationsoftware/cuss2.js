{
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": [
      "src/lib/interfaces"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "search": true
  }
}
